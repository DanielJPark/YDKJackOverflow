<% comments.each do |comment| %>
  <div class="question">
    <%= erb :'_question_title', layout: false, locals: { question: comment.post } %>
    <div class="user_highlight comments">
      <%= erb :'_comment_without_attribution', layout: false, locals: { comment: comment, question_id: comment.post.id } %>
    </div>
  </div>
<% end %>
